# Data NOT Code (nur ein Suppl., Typ Dataset)
<https://w3id.org/zpid/pub/work/0004> a bf:Work; 
    bflc:relationship 
    [
        a bflc:Relationship;
        bflc:relation relations:hasResearchData;
        bf:supplement
        [
            a bf:Work, bf:Dataset; 
            bf:genreForm genres:researchData; bf:content content:dataset;
            # move locator and doi to instance
            bf:hasInstance 
            [
                a bf:Instance;
                bf:electronicLocator <https://osf.io/data004>;
                bf:identifier [a bf:Identifier, bf:Doi; rdf:value "10.123data004"];
                # add bf:media "computer" from rda media types
                bf:media <http://rdvocab.info/termList/RDAMediaType/1003>;
                # bf:carrier "online resource" from rda vocabulary
                bf:carrier <http://rdvocab.info/termList/RDACarrierType/1018>;
                # add combined custom pxp:mediaType
                pxp:mediaType mediatypes:OnlineResource;
            ]              
        ] 
    ]
.

# gar kein supplement, aber andere relation: replikation eines bestimmten Datasets
<https://w3id.org/zpid/pub/work/0005> a bf:Work; 
        bflc:relationship [
                a bflc:Relationship;
                bflc:relation relations:isReplicationOf;
                bf:dataSource 
                [
                        a bf:Work, bf:Text; 
                        bf:genreForm genres:researchData; bf:content content:dataset;
                        # move locator and doi to instance
                        bf:hasInstance [
                                a bf:Instance;
                                bf:electronicLocator <https://osf.io/replicatedStudy>;
                                bf:identifier [a bf:Identifier, bf:Doi; rdf:value "10.123replicated"];
                                # add bf:media "computer" from rda media types
                                bf:media <http://rdvocab.info/termList/RDAMediaType/1003>;
                                # bf:carrier "online resource" from rda vocabulary
                                bf:carrier <http://rdvocab.info/termList/RDACarrierType/1018>;
                                # add combined custom pxp:mediaType
                                pxp:mediaType mediatypes:OnlineResource;
                        ]
                ] # ist Replikation dieser Daten
        ]
.
     



