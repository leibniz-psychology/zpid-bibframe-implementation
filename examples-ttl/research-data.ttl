## hat data und code in 2 separaten Supplements:
<https://w3id.org/zpid/pub/work/0001> a bf:Work; 
    bflc:relationship 
    [
        a bflc:Relationship;
        bflc:relation relations:hasResearchData;
        bf:supplement
        [
            a bf:Work, bf:Dataset; 
            bf:genreForm genres:researchData; bf:content content:dataset;
            bf:hasInstance 
            [
                a bf:Instance;
                bf:electronicLocator <https://osf.io/data001>;
                bf:identifier [a bf:Identifier, bf:Doi; rdf:value "10.123data001"]
                # "computer" from rda media types:
                bf:media <http://rdvocab.info/termList/RDAMediaType/1003>;
                # "online resource" from rda vocabulary for carrier types:
                bf:carrier <http://rdvocab.info/termList/RDACarrierType/1018>;
                # combined custom pxp:mediaType
                pxp:mediaType mediatypes:OnlineResource;
            ]
    ]
    ],
    [ # Beziehung zu Analysecode (Werk - f체r content type und genreform, Instanz f체r Links und Dois)
        a bflc:Relationship;
        bflc:relation relations:hasResearchData;
        bf:supplement
        [
            a bf:Work, bf:Multimedia; 
            bf:genreform genres:analysisCode; bf:content content:software;
                bf:hasInstance 
                [
                    a bf:Instance;
                    bf:electronicLocator <https://osf.io/code001>;
                    bf:identifier [a bf:Identifier, bf:Doi; rdf:value "10.123code001"]
                    # add bf:media "computer" from rda media types
                    bf:media <http://rdvocab.info/termList/RDAMediaType/1003>;
                    # bf:carrier "online resource" from rda vocabulary
                    bf:carrier <http://rdvocab.info/termList/RDACarrierType/1018>;
                    # add combined custom pxp:mediaType
                    pxp:mediaType mediatypes:OnlineResource;
                ] 
            ] 
    ]
.

# data AND code zusammen in einem Supplement, plus ein weiteres reines Data-Supplement
<https://w3id.org/zpid/pub/work/0002> a bf:Work; 
    bflc:relationship 
    [ # Beziehung zu FD-Supplement, das sowohl Forschungsdaten als auch Code enth채lt:
        a bflc:Relationship;
        bflc:relation relations:hasResearchData;
        bf:supplement
        [
            a bf:Work, bf:Dataset, bf:Multimedia; 
            bf:genreForm genres:researchData, genres:analysisCode; bf:content content:dataset, content:software;
            bf:hasInstance 
            [
                a bf:Instance;
                bf:electronicLocator <https://osf.io/dataCode002>;
                bf:identifier [a bf:Identifier, bf:Doi; rdf:value "10.123dataCode002"];
                # add media "computer" and carrier "online resource" from rda vocabulary
                bf:media <http://rdvocab.info/termList/RDAMediaType/1003>;
                bf:carrier <http://rdvocab.info/termList/RDACarrierType/1018>;
                # add combined custom pxp:mediaType
                pxp:mediaType mediatypes:OnlineResource;
            ]
        ]
    ],
    [ # Beziehung zu FD-Supplement, das nur Forschungsdaten enth채lt:
        a bflc:Relationship;
        bflc:relation relations:hasResearchData;
        bf:supplement
        [
            a bf:Work, bf:Multimedia; 
            bf:genreForm genres:analysisCode; bf:content content:software;
            bf:hasInstance 
            [
                a bf:Instance;
                bf:electronicLocator <https://osf.io/data002>;
                bf:identifier [a bf:Identifier, bf:Doi; rdf:value "10.123data002"];
                # add bf:media "computer" from rda media types
                bf:media <http://rdvocab.info/termList/RDAMediaType/1003>;
                # bf:carrier "online resource" from rda vocabulary
                bf:carrier <http://rdvocab.info/termList/RDACarrierType/1018>;
                # add a new property pxp:mediaType and use vocabulary mediatypes:, specifically Online Resource
                pxp:mediaType mediatypes:OnlineResource;
            ]
        ] 
    ]
.

